{
  "name": "tailwind-plugin-customcss-intellisense",
  "version": "1.0.0-rc.0",
  "description": "Enable autocompletion of custom CSS classes inside Tailwind CSS IntelliSense",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/miii/tailwind-plugin-customcss-intellisense.git"
  },
  "author": "Jacob Andersson",
  "homepage": "https://github.com/miii/tailwind-plugin-customcss-intellisense",
  "bugs": "https://github.com/miii/tailwind-plugin-customcss-intellisense/issues",
